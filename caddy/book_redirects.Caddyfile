# In January 2026, I moved all my book reviews from a separate subdomain
# (books.alexwlchan.net) to a path on the main site.
#
# Because the two sites run on the same web server, this Caddy config
# creates the redirects, so I can shut down the old site.
#
# The file `book_redirects.json` is new slug/old paths.
#
# To rebuild this file:
#
#     uv pip install cogapp
#     cog -c r caddy/books.Caddyfile
#

#[[[cog
import cog
import json

with open("caddy/book_redirects.json") as in_file:
    redirects = json.load(in_file)

cog.outl("books.alexwlchan.net {")

cog.outl("redir / https://alexwlchan.net/book-reviews/")
cog.outl("redir /reviews/ https://alexwlchan.net/book-reviews/")

for new_slug, all_old_paths in redirects.items():
    for old_path in all_old_paths:
        cog.outl(f"redir {old_path} https://alexwlchan.net/book-reviews/{new_slug}/ permanent")
  
cog.outl("}")

]]]#
